{
  "name": "microsoft-agent-framework-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent && dotnet run --launch-profile http",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "build": "next build",
    "build:worker": "opennextjs-cloudflare",
    "preview": "opennextjs-cloudflare && wrangler dev",
    "deploy": "opennextjs-cloudflare && wrangler deploy",
    "start": "next start",
    "lint": "next lint",
    "install:agent": "cd agent && dotnet restore",
    "postinstall": "npm run install:agent"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.42",
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.7",
    "@copilotkit/react-core": "1.50.0",
    "@copilotkit/react-ui": "1.50.0",
    "@copilotkit/runtime": "1.50.0",
    "@ai-sdk/openai": "^1.0.0",
    "@copilotkitnext/agent": "^1.51.3",
    "daisyui": "^5.5.18",
    "next": "16.0.8",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
